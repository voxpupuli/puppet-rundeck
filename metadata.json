{
  "name":         "puppet-rundeck",
  "version":      "3.0.0",
  "author":       "voxpupuli",
  "license":      "Apache-2.0",
  "summary":      "Module for managing the installatation and configuration of the rundeck orchestration tool",
  "source":       "https://github.com/voxpupuli/puppet-rundeck.git",
  "project_page": "https://github.com/voxpupuli/puppet-rundeck",
  "issues_url":   "https://github.com/voxpupuli/puppet-rundeck/issues",
  "tags": [
    "orchestration",
    "rundeck",
    "voxpupuli"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5.0","6.0","7.0"]
    },
    { "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5.0","6.0", "7.0"]
    },
    { "operatingsystem": "Debian",
      "operatingsystemrelease": ["6.0","7.0", "8.0"]
    },
    { "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04", "16.04"]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.6.0 <5.0.0"
    },
    {
      "name": "pltraining/dirtree"
    },
    {
      "name": "puppetlabs/inifile",
      "version_requirement": ">= 1.0.3 <2.0.0"
    },
    {
      "name": "puppet/archive",
      "version_requirement": ">= 0.3.0 <1.0.0"
    }
  ]
}
